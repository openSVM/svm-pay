{
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-console": "off",
    "no-undef": "off",
    "no-redeclare": "off"
  },
  "ignorePatterns": [
    "dist/",
    "node_modules/",
    "website/"
  ]
}